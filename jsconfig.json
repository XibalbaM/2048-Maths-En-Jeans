{
  "compilerOptions": {
    "checkJs": true,
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "lib": ["ES2020", "DOM"],
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "baseUrl": ".",
    "paths": {
      "*": ["*"]
    }
  },
  "include": [
    "**/*.js",
    "app/**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
